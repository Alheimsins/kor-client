{
  "name": "@alheimsins/kor-client",
  "version": "1.0.2",
  "description": "Module for authenticate with oauth2 and retreiving data from kontakt- og reservasjonsregisteret",
  "main": "src/index.js",
  "files": [
    "src/*"
  ],
  "engines": {
    "node": ">=10.15.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "standard && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "difi",
    "kontakt og reservasjonsregisteret",
    "kor"
  ],
  "author": {
    "name": "Jonas Maccyber Enge",
    "email": "jonas.enge@gmail.com",
    "url": "https://github.com/maccyber"
  },
  "contributors": [
    {
      "name": "Geir GÃ¥sodden",
      "email": "geir.gasodden@pythonia.no",
      "url": "https://github.com/zrrrzzt"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alheimsins/kor-client.git"
  },
  "bugs": {
    "url": "https://github.com/alheimsins/kor-client/issues"
  },
  "homepage": "https://github.com/alheimsins/kor-client#readme",
  "dependencies": {
    "axios": "0.19.1",
    "jsonwebtoken": "8.5.1",
    "uuid-random": "1.3.0"
  },
  "devDependencies": {
    "ava": "2.4.0",
    "coveralls": "3.0.9",
    "husky": "4.0.1",
    "nyc": "15.0.0",
    "standard": "14.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run standard-fix && npm test"
    }
  }
}
